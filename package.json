  "private": true,
  "dependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
